<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="350.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginPage">
    <children>
       <Pane prefHeight="350.0" prefWidth="450.0">
          <children>
             <TextField fx:id="txtUserName" layoutX="96.0" layoutY="84.0" prefHeight="41.0" prefWidth="266.0" promptText="UserName" />
             <PasswordField fx:id="txtPassword" layoutX="96.0" layoutY="136.0" prefHeight="41.0" prefWidth="266.0" promptText="Password" />
             <Button fx:id="btnLogin" graphicTextGap="5.0" layoutX="159.0" layoutY="224.0" mnemonicParsing="false" onAction="#Login" prefHeight="31.0" prefWidth="132.0" text="Login" />
             <Button fx:id="btnNewAccount" graphicTextGap="5.0" layoutX="159.0" layoutY="263.0" mnemonicParsing="false" onAction="#NewAccount" prefHeight="31.0" prefWidth="132.0" text="New Registration" />
             <Label contentDisplay="CENTER" graphicTextGap="10.0" layoutX="129.0" layoutY="29.0" prefHeight="31.0" prefWidth="214.0" text="Medical Information" textAlignment="CENTER" textFill="#291ac4" underline="true">
                <font>
                   <Font size="21.0" />
                </font>
             </Label>
             <Label fx:id="lblStatus" layoutX="96.0" layoutY="185.0" prefHeight="22.0" prefWidth="266.0">
                <font>
                   <Font size="14.0" />
                </font>
             </Label>
          </children>
       </Pane>
    </children>
 </StackPane>
